
@{
    ViewBag.Title = "PushDocument";
}

<h2>Push document</h2>
<div>
    <form>
        <div class="form-group">
            <label for="edDocumentsId">DocumentsId</label>
            <input class="form-control" id="edDocumentsId" placeholder="DocumentsId" value="1">
        </div>
        <div class="form-group">
            <label for="eddfPathFoldersId">dfPathFoldersId</label>
            <input class="form-control" id="eddfPathFoldersId" value="251">
        </div>
        <div class="form-group">
            <label for="eddfMethodsIdTarget">dfMethodsIdTarget</label>
            <input class="form-control" id="eddfMethodsIdTarget" value="139">
        </div>

        <div class="form-group">
            <label for="edDbName">DBName</label>
            <input class="form-control" id="edDbName" value="DocFlow">
        </div>

        <button type="submit" class="btn btn-primary" onclick="TryPushDocument()">Push</button>
    </form>

</div>


<div class="container-fluid">
    <label for="memoResult">Result</label>
    <textarea class="form-control" id="memoResult" rows="10"></textarea>
</div>